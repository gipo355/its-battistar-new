{
  "name": "its-battistar-be",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/its-battistar-be/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "its-battistar-be:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "its-battistar-be:build:development"
        },
        "production": {
          "buildTarget": "its-battistar-be:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/its-battistar-be/jest.config.ts"
      }
    },
    "docker-build": {
      "dependsOn": ["build"],
      "command": "docker build -f apps/its-battistar-be/Dockerfile . -t its-battistar-be"
    }
  }
}
