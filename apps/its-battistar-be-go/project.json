{
  "name": "its-battistar-be-go",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/its-battistar-be-go/cmd/server",
  "root": "apps/its-battistar-be-go",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go build -o=../../dist/apps/its-battistar-be-go/ ./cmd/server",
        "cwd": "apps/its-battistar-be-go"
      }
    },
    "docker-build": {
      "dependsOn": ["build"],
      "command": "docker build -f apps/its-battistar-be-go/Dockerfile . -t its-battistar-be-go"
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go-task serve-api",
        "cwd": "apps/its-battistar-be-go"
      }
    },
    "init": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go-task init",
        "cwd": "apps/its-battistar-be-go"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go-task dev",
        "cwd": "apps/its-battistar-be-go"
      }
    }
  }
}
